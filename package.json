{
  "scripts": {
    "start": "node src/app.js",
    "clean": "rm -rf ./public/dist",
    "dev": "concurrently \"nodemon src/app.js\" \"cd src/client && npm start\"",
    "dev:debug": "nodemon --inspect-brk src/app.js",
    "console": "node --experimental-repl-await ./src/console.js",
    "migrate:latest": "knex --knexfile ./knexfile.cjs migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.cjs migrate:rollback",
    "migrate:make": "knex --knexfile ./knexfile.cjs migrate:make",
    "db:test:migrate": "NODE_ENV='test' yarn run migrate:latest",
    "db:e2e:migrate": "NODE_ENV='e2e' yarn run migrate:latest",
    "test": "jest",
    "ci": "jest --coverage",
    "generate-secret": "./scripts/generate-secret.js",
    "dev:e2e": "NODE_ENV=e2e PORT=8765 nodemon src/app.js",
    "sendMessage": "node src/utils/messageSend.js",
    "install_client": "cd src/client && npm install",
    "build_client": "cd src/client && npm run build",
    "test_baystate": "node ./src/apiClient/crawlBaystate.js",
    "update_auctions": "node ./src/apiClient/scrap.js",
    "postinstall": "npm install --force",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run install_client && npm run build_client"
  },
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "name": "auction-site-server",
  "engines": {
    "node": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "babel-jest": "^27.0.2",
    "dotenv": "^8.6.0",
    "errorhandler": "^1.5.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "install-peerdeps": "^3.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "webpack": "^5.3.2",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.6",
    "concurrently": "^7.1.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "heroku-ssl-redirect": "^0.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "node-cron": "^3.0.0",
    "node-geocoder": "^3.27.0",
    "objection": "^2.2.15",
    "objection-unique": "^1.2.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "puppeteer": "^8.0.0",
    "react-helmet": "^6.1.0",
    "regenerator-runtime": "^0.13.7",
    "selenium-webdriver": "^4.16.0",
    "string-similarity": "^4.0.4",
    "twilio": "^3.75.0",
    "undici": "^5.0.0"
  }
}
